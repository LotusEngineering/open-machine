    "version": "0.2.0",
    "configurations": [
        {
            "name": "Blinky-h743-Debug",
            "cwd": "${workspaceFolder}/code/examples/blinky/blinky_h743/",
            "executable": "./build/blinky_h743.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "stlink",
            "preLaunchTask": "Blinky H743 Make All"
        },
        {
            "name": "Tea Timer-h743-Debug",
            "cwd": "${workspaceFolder}/code/examples/tea_timer/tea_timer_h743/",
            "executable": "./build/tea_timer_h743.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "stlink",
            "preLaunchTask": "Tea Timer H743 Make All"
        },
        {
            "name": "HSM_Test-mingw-Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/code/test/hsm_test/build/hsm_test.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "C:/MinGW/bin/gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "HSM_Test-Build"

        },
        {
            "name": "Blinky-Posix-Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/code/examples/blinky/blinky_posix/build/blinky_posix",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Blinky-Posix-Build"

        }
    ]
}